{
	"name": "@medovnik/npm-proj",
	"version": "1.0.3",
	"main": "index.js",
	"module": "./build/bundle.mjs",
	"license": "MIT",
	"type": "module",
	"types": "./build/types/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./build/types/index.d.ts",
				"default": "./build/bundle.mjs"
			},
			"require": {
				"types": "./build/types/index.d.ts",
				"default": "./build/bundle.cjs"
			}
		}
	},
	"sideEffects": false,
	"engines": {
		"node": ">=18.12"
	},
	"scripts": {
		"dev": "npx nodemon",
		"build": "rimraf ./build && rollup -c && tsc",
		"start": "npm run build && node build/index.js",
		"typecheck": "tsc --noEmit",
		"lint": "eslint . --ext .ts",
		"prettier:check": "prettier --config .prettierrc 'src/**/*.ts' --check",
		"types:generate": "tsc --declaration"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@types/node": "^20.11.17",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"esbuild": "^0.20.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"nodemon": "^3.0.3",
		"prettier": "^3.2.5",
		"rimraf": "^5.0.5",
		"rollup": "^4.10.0",
		"typescript": "^5.3.3"
	},
	"packageManager": "yarn@4.1.0",
	"author": "",
	"description": ""
}
